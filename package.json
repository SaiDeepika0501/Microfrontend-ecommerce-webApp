{
  "name": "mfe-store",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:products\" \"npm run start:container\"",
    "start:products": "cd products && npm start",
    "start:container": "cd container && npm start",
    "build": "concurrently \"npm run build:products\" \"npm run build:container\"",
    "build:products": "cd products && npm run build",
    "build:container": "cd container && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
